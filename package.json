{
  "name": "frontm8er",
  "version": "3.1.2",
  "type": "module",
  "engines": {
    "node": ">= 22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/terabaud/frontm8er"
  },
  "description": "A tool to add frontmatter to markdown files",
  "main": "dist/frontm8er.js",
  "module": "dist/frontm8er.mjs",
  "types": "dist/frontm8er.d.ts",
  "exports": {
    ".": {
      "require": "./dist/frontm8er.js",
      "import": "./dist/frontm8er.mjs"
    }
  },
  "bin": {
    "frontm8er": "dist/cli.js"
  },
  "files": [
    "dist/cli.js",
    "dist/frontm8er.js",
    "dist/frontm8er.mjs",
    "dist/frontm8er.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "start": "ts-node src/cli.ts --help",
    "build:types": "tsc -t esnext --esModuleInterop --allowSyntheticDefaultImports --resolveJsonModule --moduleResolution node -d --emitDeclarationOnly --outFile dist/frontm8er.d.ts src/frontm8er.ts",
    "build:bundles": "rollup -c rollup.config.js",
    "build": "npm run build:types -s && npm run build:bundles -s",
    "lint": "eslint src/**/*.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "batch-processing",
    "command-line",
    "markdown",
    "metadata"
  ],
  "author": "Lea Rosema",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/learosema"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/chokidar": "^2.1.7",
    "@types/glob": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@types/json5": "2.2.0",
    "@types/minimist": "^1.2.5",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-plugin-jest": "^29.12.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^30.2.0",
    "jest-mock": "^30.2.0",
    "rollup": "^4.55.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "json5": "^2.2.3",
    "minimist": "^1.2.8",
    "yaml": "^2.8.2"
  }
}
