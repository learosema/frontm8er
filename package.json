{
  "name": "frontm8er",
  "version": "3.1.0",
  "engines": {
    "node": ">= 11.14.0"
  },
  "description": "A tool to add frontmatter to markdown files",
  "main": "dist/frontm8er.js",
  "module": "dist/frontm8er.mjs",
  "types": "dist/frontm8er.d.ts",
  "exports": {
    ".": {
      "require": "./dist/frontm8er.js",
      "import": "./dist/frontm8er.mjs"
    }
  },
  "bin": {
    "frontm8er": "dist/cli.js"
  },
  "files": [
    "dist/cli.js",
    "dist/frontm8er.js",
    "dist/frontm8er.mjs",
    "dist/frontm8er.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "start": "ts-node -r esm src/cli.ts --help",
    "build:types": "tsc -t esnext --esModuleInterop --allowSyntheticDefaultImports --resolveJsonModule --moduleResolution node -d --emitDeclarationOnly --outFile dist/frontm8er.d.ts src/frontm8er.ts",
    "build:bundles": "rollup -c rollup.config.js",
    "build": "npm run build:types -s && npm run build:bundles -s",
    "lint": "eslint src/**/*.ts",
    "test": "jest --coverage"
  },
  "keywords": [
    "batch-processing",
    "command-line",
    "markdown",
    "metadata"
  ],
  "author": "Lea Rosema",
  "funding": {
    "type": "individual",
    "url": "https://ko-fi.com/learosema"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/chokidar": "^2.1.3",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.19",
    "@types/json5": "0.0.30",
    "@types/node": "^14.14.14",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "rollup": "^2.35.1",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@rollup/plugin-typescript": "^8.1.0",
    "@types/minimist": "^1.2.1",
    "chokidar": "^3.4.3",
    "eslint-plugin-node": "^11.1.0",
    "esm": "^3.2.25",
    "glob": "^7.1.6",
    "json5": "^2.1.3",
    "minimist": "^1.2.5",
    "yaml": "^1.10.0"
  }
}
